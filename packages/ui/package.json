{
  "name": "@pnpm-workspace/ui",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./composables": {
      "types": "./src/composables/index.ts",
      "import": "./src/composables/index.ts"
    },
    "./themes/*": "./src/themes/*.css"
  },
  "scripts": {
    "dev": "pnpm run \"/^dev:.*/\"",
    "build": "pnpm type-check",
    "type-check": "vue-tsc",
    "dev:type-check": "pnpm type-check --watch",
    "dev:storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "class-variance-authority": "0.7.0",
    "vue": "3.4.21"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "8.0.5",
    "@storybook/addon-interactions": "8.0.5",
    "@storybook/addon-links": "8.0.5",
    "@storybook/blocks": "8.0.5",
    "@storybook/preview-api": "8.0.5",
    "@storybook/test": "8.0.5",
    "@storybook/vue3": "8.0.5",
    "@storybook/vue3-vite": "8.0.5",
    "@vitejs/plugin-vue": "5.0.4",
    "storybook": "8.0.5",
    "vue-tsc": "2.0.7"
  }
}
